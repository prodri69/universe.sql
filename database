CREATE TABLE moon (
    moon_id SERIAL PRIMARY KEY,
    Name VARCHAR(30) NOT NULL,
    Type TEXT,
    Radius_in_km NUMERIC,
    Planet_id INT REFERENCES Planet(Planet_id) NOT NULL,
    population INT UNIQUE,
    Has_water BOOLEAN
);



CREATE TABLE moon_2 (
    moon_2_id SERIAL PRIMARY KEY,
    Name VARCHAR(30) NOT NULL,
    Type TEXT,
    Radius_in_km NUMERIC,
    Planet_id INT REFERENCES Planet(Planet_id) NOT NULL,
    population INT UNIQUE,
    Has_water BOOLEAN
);

