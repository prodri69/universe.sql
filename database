(3, 'Betelgeuse', 'M1-2la', 1.989.000.000.000.000.000.000.000.000, 700.000.000, 1, 0, false),
(4, 'Proxima Centauri, 'M5.5V', 238.680.000.000.000.000.000.000.000.000, 97.286, 1, 0, false),
(5, 'M31 RV', 'Unknown', 0, 0, 2, 0, false),
(6, 'M51', 'Unknown', 0, 0, 5, 0, false);


CREATE TABLE planet (
    planet_id SERIAL PRIMARY KEY,
    Name VARCHAR(30) NOT NULL,
    Type TEXT,
    Mass_in_kg NUMERIC,
    Star_id INT REFERENCES Star(Star_id) NOT NULL,
    population INT UNIQUE,
    Has_water BOOLEAN
);

